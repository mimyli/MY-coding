public class Pisin {
    public static void main(String[] args) {
        if (args.length == 0) {
            System.out.println("Ei parametreja"); 
        }
        if (args.length >= 1) {
            String longestArg = "";
            for (int i = 0; i < args.length; i++) {
                if (args[i].length() > longestArg.length()) {
                    longestArg = args[i];
                }
            }
            System.out.println("Pisin parametri: " + longestArg);
        }
    }
}
